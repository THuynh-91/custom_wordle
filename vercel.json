{
    "version":  2,
    "buildCommand":  "npm run build",
    "outputDirectory":  "dist/frontend",
    "functions":  {
                      "api/index.js":  {
                                           "includeFiles":  "dist/**"
                                       }
                  },
    "routes":  [
                   {
                       "src":  "^/health$",
                       "dest":  "api/index.js"
                   },
                   {
                       "src":  "^/api/(.*)$",
                       "dest":  "api/index.js"
                   },
                   {
                       "src":  "^/(.*)$",
                       "dest":  "/index.html"
                   }
               ]
}
